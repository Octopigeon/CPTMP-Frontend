.dialog-actions
  justify-content flex-end

.content-wrapper
  max-width calc(80vw - 48px)
  width 300px
  margin 30px 0
  display flex
  flex-direction column
  justify-content center
  align-items center

  .icon
    height 48px
    width 48px
    margin-bottom 20px

    .mat-icon
      line-height 48px
      height 48px
      width 48px
      font-size 48px

  .info
    word-break keep-all
